{
  "compilerOptions": {
    "strict": true,
    "target": "esnext",
    "module": "node16",
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
